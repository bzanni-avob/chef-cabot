upstream django {
    server 127.0.0.1:5000;
 }
 server {
   listen 0.0.0.0:80;
   server_name monitor.lan2;
   access_log /var/www/logs/cabot.access.log;
   error_log /var/www/logs/cabot.error.log;

    location / {  
      uwsgi_pass django;
      include        uwsgi_params;
   }

   location /static/ {
     alias /var/www/cabot/static/;
   }
 }